openssl req -new -nodes -newkey rsa:4096 -keyout tls.key -out tls.csr -batch -subj "/C=NL/ST=Noord-Brabant/L=Eindhoven/O=CloudShapers/CN=vault.cloudshaperstest.nl/emailAddress=rick.moonen@cloudshapers.nl" -reqexts SAN -config <(cat /etc/pki/tls/openssl.cnf <(printf "[SAN]\nsubjectAltName=IP:127.0.0.1,DNS:localhost,DNS:vault.cloudshaperstest.nl,DNS:*.vault.cloudshaperstest.nl"))